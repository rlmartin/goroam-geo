<item <%= render(:partial => 'items/lat_lng', :locals => { :geo_point => defined?(geo_point)? geo_point : nil, :item => item }).chomp %> title='<%= item.title %>' type='<%= item.data_type.name %>'<% if defined? dist_geo_point %> dist='<%= item.distance_to(dist_geo_point, { :units => units }) %>'<% end %>><%= h(item.value) %></item>
